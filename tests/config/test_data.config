
params {
    // Default endpoint provided by nf-core, we keep it here in case sometimes we end up pushing stuff to them
    test_data_base = "https://github.com/scilus/nf-scil/raw"
    test_data_normalize = "https://github.com/Manonedde/nf-scil/raw"

    test_data {
        "scilpy" {
            "cropvolume" {
                image = "${params.test_data_base}/main/.test_data/scilpy/crop_volume_image.nii.gz"
                mask = "${params.test_data_base}/main/.test_data/scilpy/crop_volume_mask.nii.gz"
            }
        }
        "preproc" {
            "normalize"{
                dwi = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize.nii.gz"
                mask = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize_mask.nii.gz"
                bval = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize_bval.bval"
                bvec = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize_bvec.bvec"
            }
        }
    }
}
