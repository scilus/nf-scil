
params {
    // Default endpoint provided by nf-core, we keep it here in case sometimes we end up pushing stuff to them
    test_data_base = "https://github.com/scilus/nf-scil/raw"
    test_data_normalize = "https://github.com/Manonedde/nf-scil/raw"

    test_data {
        "betcrop" {
            "fslbetcrop" {
                dwi = "${params.test_data_base}/main/.test_data/fslbetcrop/fslbetcrop_dwi.nii.gz"
                bval = "${params.test_data_base}/main/.test_data/fslbetcrop/fslbetcrop_bval.bval"
                bvec = "${params.test_data_base}/main/.test_data/fslbetcrop/fslbetcrop_bvec.bvec"
            }
        }
        "scilpy" {
            "cropvolume" {
                image = "${params.test_data_base}/main/.test_data/scilpy/dwi.nii.gz"
                mask = "${params.test_data_base}/main/.test_data/scilpy/mask.nii.gz"
            }
        }
        "preproc" {
            "n4" {
                dwi = "${params.test_data_base}/main/.test_data/preproc/dwi.nii.gz"
                b0 = "${params.test_data_base}/main/.test_data/preproc/b0.nii.gz"
                b0_mask = "${params.test_data_base}/main/.test_data/preproc/b0_mask.nii.gz"
            }
        }
        "denoising" {
            "nlmeans"{
                image = "${params.test_data_base}/main/.test_data/denoising/nlmeans_image.nii.gz"
                mask = "${params.test_data_base}/main/.test_data/denoising/nlmeans_mask.nii.gz"
            }
        }
        "preproc" {
            "normalize"{
                dwi = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize.nii.gz"
                mask = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize_mask.nii.gz"
                bval = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize_bval.bval"
                bvec = "${params.test_data_normalize}/add_normalize/.test_data/preproc/dwi_normalize_bvec.bvec"
            }
        }
    }
}
